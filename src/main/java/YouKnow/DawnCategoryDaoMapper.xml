<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myhealingbox.dao.dawn.DawnCategoryDao">

	<select id="getList" resultType="myhealingbox.entity.dawn.DawnView">
		select * from DawnView order by DCRegDate desc
		<!-- limit ${(_parameter-1)*15},15 -->
	</select>
	
	<!-- <select id="get" resultType="myhealingbox.entity.dawn.DawnView">
		select * from DawnView where DCId=#{id}
	</select> -->
	
	<insert id="insert" parameterType="myhealingbox.entity.dawn.DawnCategory">
		insert into DawnCategory(title)
		values(#{title})
	</insert>
	
	<update id="update" parameterType="myhealingbox.entity.dawn.DawnCategory">
		update DawnCategory set title=#{title} where id=#{id}
	</update>
	
	<delete id="delete" parameterType="Integer">
		delete from DawnCategory where id=#{id}
	</delete>

</mapper>